class Myself {  
    var firstname: String = "Anshita"
    var secondname: String = "Sharma"

    init {  
       println(firstname)  
    }  
  
    init {  
       println(secondname) 
    }  
  
    companion object {  
    fun age():Int {
        return 24;
        }
     }  
  
}  
fun main(args: Array<String>){  
    val obj = Myself()
    obj.age()
}  
