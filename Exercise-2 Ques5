
fun main(args:Array<String>){
  val numbers = intArrayOf(1, 2, 3)
  var list:MutableList<String> = printNumbers(*numbers)
  println(list)
}

fun printNumbers(vararg numbers: Int):MutableList<String> {
  
    var list:MutableList<String>
    for (number in numbers) {
        if(number in 50..60)
          list.add("Good")
        else if(number in 60..70)
          list.add("Very Good")
        else if(number in 70..80)
          list.add("Excellent")
        else if(number in 80..100)
          list.add("Rockstar")
    }
    return list
}
